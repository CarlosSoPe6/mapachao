    "mongoose": "^5.12.3"
  },
  "devDependencies": {
    "env-cmd": "^10.1.0",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7"
  }
}
